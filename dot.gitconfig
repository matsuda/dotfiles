[user]
	name = Kosuke Matsuda
	email = 
[github]
	user = matsuda
	token = 
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = auto
[alias]
	co = checkout
	ci = commit -a
	st = status
	up = pull --rebase
	a  = add
	b  = branch
	c  = commit
	d  = diff
	info = config --list
	staged = diff --cached
	unstaged = diff
	both = diff HEAD
	oneline = log --pretty=oneline --abbrev-commit
	amend = commit --amend
[apply]
	whitespace = nowarn
[core]
	excludesfile = /Users/matsuda/.gitignore
